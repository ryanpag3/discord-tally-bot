{
    "name": "discord-tally-bot",
    "version": "1.3",
    "description": "A discord bot for tallying things.",
    "main": "dist/bot.js",
    "scripts": {
        "build": "tsc",
        "start": "ts-node src/bot.ts",
        "start-prod": "node -r ts-node/register --max-old-space-size=1500 src/bot.ts",
        "shard-prod": "ts-node -T src/shard-manager.ts",
        "watch": "tsnd --watch --notify=false src/bot.ts",
        "test": "export TEST_ENV=\"1\" && ts-mocha -r dotenv/config --recursive --exit --timeout 5000 ./**/*.test.ts",
        "coverage": "export TEST_ENV=\"1\" && nyc mocha -r dotenv/config --recursive --exit --timeout 5000 ./**/*.test.ts",
        "run-test": "export TEST_ENV=\"1\" && ts-mocha -r dotenv/config --recursive --exit --timeout 5000"
    },
    "repository": {
        "type": "git",
        "url": "-"
    },
    "author": "Ryan Page",
    "license": "ISC",
    "dependencies": {
        "axios": "^0.19.2",
        "chrono-node": "^1.3.5",
        "cron": "^1.6.0",
        "cron-parser": "^2.7.3",
        "dblapi.js": "^2.3.0",
        "dedent-js": "^1.0.1",
        "discord.js": "^11.4.2",
        "dotenv": "^8.2.0",
        "erlpack": "github:discordapp/erlpack",
        "later": "^1.2.0",
        "log4js": "^6.1.0",
        "minimist": "1.2.3",
        "moment-timezone": "^0.5.23",
        "mysql": "^2.16.0",
        "mysql2": "^1.6.1",
        "sequelize": "4.44.3",
        "source-map-support": "^0.5.12",
        "table": "^5.1.0",
        "ts-node": "^8.9.0",
        "ts-node-dev": "^1.0.0-pre.44",
        "typescript": "^3.1.3"
    },
    "devDependencies": {
        "@types/chai": "^4.2.5",
        "@types/mocha": "^5.2.7",
        "@types/mysql": "^2.15.8",
        "@types/sequelize": "^4.28.7",
        "chai": "^4.2.0",
        "mocha": "^6.2.2",
        "nyc": "^14.1.1",
        "randomstring": "^1.1.5",
        "sinon": "^7.5.0",
        "ts-mocha": "^6.0.0"
    },
    "nyc": {
        "include": [
            "**/*.ts",
            "**/*.tsx"
        ],
        "exclude": [
            "**/*.test.ts"
        ],
        "extension": [
            ".ts",
            ".tsx"
        ],
        "require": [
            "ts-node/register"
        ],
        "sourceMap": true,
        "instrument": true,
        "all": true,
        "reporter": [
            "text",
            "html"
        ]
    }
}
